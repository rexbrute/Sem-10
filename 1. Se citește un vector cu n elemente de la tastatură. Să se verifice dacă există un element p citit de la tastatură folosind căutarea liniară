#include <stdio.h>
#include <stdlib.h>

int main()
{ int n,i,p,a[50],ok=0;;
printf("n=");
scanf("%d",&n);
printf("p=");
scanf("%d",&p);
for(i=0;i<n;i++)
{scanf("%d",&a[i]);}
    while ( i<n && a[i]!=p)
 {i++;
 if(a[i] != p)
ok=1;
else
ok=0;}
if(ok==1)
 printf("Valoarea  nu a fost gasita");
else
printf("Valoarea %d a fost gasita  ",p);
    return 0;
}
